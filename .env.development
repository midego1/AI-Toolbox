# Development Environment Variables
# Copy this to .env.local for local development

# Database (use local PostgreSQL or Neon free tier for dev)
DATABASE_URL="postgresql://user:password@localhost:5432/ai_toolbox_dev?schema=public"

# NextAuth (for local development)
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="development-secret-change-in-production"

# Stripe Test Keys (get from https://dashboard.stripe.com/test/apikeys)
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# Stripe Test Price IDs (create in Stripe Dashboard)
STRIPE_PRICE_ID_PRO_MONTHLY="price_test_..."
STRIPE_PRICE_ID_PRO_YEARLY="price_test_..."
STRIPE_PRICE_ID_ENTERPRISE_MONTHLY="price_test_..."
STRIPE_PRICE_ID_ENTERPRISE_YEARLY="price_test_..."

# AI Service API Keys (use test/development keys)
OPENAI_API_KEY="sk-..."
ANTHROPIC_API_KEY="sk-ant-..."
GOOGLE_CLOUD_VISION_API_KEY="..."
DEEPL_API_KEY="..."
REPLICATE_API_TOKEN="r8_..."

# File Storage (optional for development)
STORAGE_BUCKET_NAME="ai-toolbox-dev"
STORAGE_ACCESS_KEY="..."
STORAGE_SECRET_KEY="..."
STORAGE_REGION="auto"
STORAGE_ENDPOINT="..."
